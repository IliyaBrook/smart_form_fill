{
  "name": "smart-form-fill-react-vite",
  "version": "0.0.1",
  "description": "chrome extension boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "cross-env __DEV__=true vite build -w",
    "build:hmr": "node src/utils/reload/rollup.config.mjs",
    "wss": "node src/utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)"
  },
  "type": "module",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.2.2",
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.224",
    "@types/node": "18.15.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.2.0",
    "antd": "4.24.16",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "11.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "rollup": "4.22.4",
    "sass": "1.62.1",
    "sweetalert2": "^11.17.2",
    "tailwindcss": "^3.3.2",
    "ts-loader": "9.4.2",
    "ts-node": "^10.9.2",
    "typescript": "4.8.3",
    "vite": "6.0.9",
    "ws": "8.17.1"
  }
}
